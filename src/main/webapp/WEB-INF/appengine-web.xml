<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>give-your-feedback</application>
    <version>3</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <static-files>
        <include path="/favicon.ico" expiration="30d"/>
        <include path="/apple-touch-icon.png" expiration="30d"/>
        <include path="/apple-touch-icon-precomposed.png" expiration="30d"/>
        <!--
        <include path="/css/**.css" expiration="12h" />
        <include path="/js/**" expiration="12h" />
        -->
    </static-files>
</appengine-web-app>
